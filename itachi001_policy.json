{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": "s3:ListBucket",
            "Resource": "arn:aws:s3:::s3-putty-log-001",
            "Condition": {
                "StringLike": {
                    "s3:prefix": [
                        "putty",
                        "putty/*"
                    ]
                }
            }
        },
        {
            "Effect": "Allow",
            "Action": "s3:*",
            "Resource": [
                "arn:aws:s3:::s3-putty-log-001",
                "arn:aws:s3:::3-putty-log-001/putty-log/*"
            ]
        }
    ]
}
